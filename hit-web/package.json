{
  "scripts": {
    "build": "npm run build:sass && npm run build:elm-land",
    "build:elm-land": "elm-land build",
    "build:sass": "sass --load-path=node_modules/@picocss/pico/scss sass:static --style=compressed --no-source-map",
    "dev": "npm run dev:sass & npm run dev:elm-land",
    "dev:sass": "sass --load-path=node_modules/@picocss/pico/scss sass:static --watch",
    "dev:elm-land": "elm-land server",
    "format": "elm-format --yes src/",
    "start": "npm run dev"
  },
  "devDependencies": {
    "@picocss/pico": "^2.1.1",
    "elm": "^0.19.1-6",
    "elm-format": "^0.8.8",
    "elm-land": "^0.20.1",
    "sass": "^1.97.1"
  }
}
